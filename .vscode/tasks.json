{
    "version": "2.0.0",
    "presentation": {
        "reveal": "always",
        "panel": "new"
    },
    "tasks": [
        {
            "taskName": "cargo build",
            "type": "shell",
            "command": "cargo",
            "args": [
                "build"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$rustc"
        },
        {
            "taskName": "cargo run",
            "type": "shell",
            "command": "cargo",
            "args": [
                "run"
            ],
            "problemMatcher": "$rustc"
        },
        {
            "taskName": "cargo test",
            "type": "shell",
            "command": "cargo",
            "args": [
                "test"
            ],
            "group": {
                "kind": "test",
                "isDefault": true
            },
            "problemMatcher": "$rustc"
        },
        {
            "taskName": "cargo clean",
            "type": "shell",
            "command": "cargo",
            "args": [
                "clean"
            ]
        },
        {
            "taskName": "extract_cldr",
            "type": "process",
            "command": "./scripts/extract_cldr/extract_cldr.py",
            "problemMatcher": null
        }
    ]
}